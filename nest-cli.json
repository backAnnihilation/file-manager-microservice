{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "fileHub/src",
  "root": "fileHub",
  "monorepo": false,
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "fileHub/tsconfig.app.json"
  },
  "projects": {
    "fileHub": {
      "type": "application",
      "root": "fileHub",
      "entryFile": "main",
      "sourceRoot": "fileHub/src",
      "compilerOptions": {
        "tsConfigPath": "fileHub/tsconfig.app.json"
      }
    }
  }
}
